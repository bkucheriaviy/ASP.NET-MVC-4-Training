@model MvcModels.Models.Person

@{
    ViewBag.Title = "Index";
}

<h2>Display Person</h2>
<div>@Html.LabelFor(m => m.PersonId)@Html.DisplayFor(m => m.PersonId)</div>
<div>@Html.LabelFor(m => m.FirstName)@Html.DisplayFor(m => m.FirstName)</div>
<div>@Html.LabelFor(m => m.LastName)@Html.DisplayFor(m => m.LastName)</div>
<div>@Html.LabelFor(m => m.Role)@Html.DisplayFor(m => m.Role)</div>
<div>@Html.LabelFor(m => m.BirthDate)@Html.DisplayFor(m => m.BirthDate)</div>
<div>@Html.LabelFor(m => m.Address.City)@Html.DisplayFor(m => m.Address.City)</div>
<div>@Html.LabelFor(m => m.Address.Country)@Html.DisplayFor(m => m.Address.Country)</div>
